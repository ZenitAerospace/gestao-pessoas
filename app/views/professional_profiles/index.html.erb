<p id="notice"><%= notice %></p>

<h1>Listing Professional Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professional_profiles.each do |professional_profile| %>
      <tr>
        <td><%= link_to 'Show', professional_profile %></td>
        <td><%= link_to 'Edit', edit_professional_profile_path(professional_profile) %></td>
        <td><%= link_to 'Destroy', professional_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professional profile', new_professional_profile_path %>
