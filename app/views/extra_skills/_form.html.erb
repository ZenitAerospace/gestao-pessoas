<%= form_for([@professional_profile.user,@professional_profile,@extra_skill]) do |f| %>
 <%= render 'layouts/error_messages', object: f.object %>
 <div class="form-group">
   <%= f.label "Nome", class: 'col-xs-3' %>
   <div class='col-xs-9'>
     <%= f.text_field :name, class: "form-control" %>
   </div>
 </div> 
 <div class="form-group">
   <%= f.label "Nivel", class: 'col-xs-3' %>
   <div class='col-xs-9'>
     <%= f.select :level, selection_level,{},{:class => "btn btn-default dropdown-toggle"} %>
   </div>
 </div>
  <br><br><br><br><br><br><br><br><br> 
  <div class="actions">
    <%= f.submit  class:  "btn btn-default"%>
  </div>
<% end %>
