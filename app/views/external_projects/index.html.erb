<p id="notice"><%= notice %></p>

<h1>Listing External Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @external_projects.each do |external_project| %>
      <tr>
        <td><%= link_to 'Show',  user_professional_profile_external_project_path(@professional_profile.user,@professional_profile,external_project)  %></td>
        <td><%= link_to 'Edit', edit_user_professional_profile_external_project_path(@professional_profile.user,@professional_profile,external_project) %></td>
        <td><%= link_to 'Destroy', external_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New External project', new_external_project_path %>
